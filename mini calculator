# mini project for calculator
N1=int(input("enter a number"))
N2=int(input("enter a number"))
# addition
print("()+()=".formate(N1,N2))
print(N1+N2)
# Substraction
print("()-()=".formate(N1,N2))
print(N1-N2)
# MUultipilcation
print("()*()=".formate(N1,N2))
print("N1*N2")
# Division
print("()/()=".formte(N1,N2))
print("N1/N2")
